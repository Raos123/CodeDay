.row
  .span6.offset3
    = form_for(@project) do |f|
      = render 'shared/error_messages', object:@project
            
      = f.label :name
      = f.text_field :name
      
      = f.label :company_id
      = f.select :company_id, Company.all.map {|c| [c.name, c.id]}
      
      = f.submit class: "btn btn-large btn-primary"
